{
  "name": "bitloops-language",
  "displayName": "Bitloops Language",
  "description": "Official Bitloops Language Extension",
  "publisher": "Bitloops",
  "repository": {
    "type": "git",
    "url": "git@github.com:bitloops/bitloops-language-vscode.git"
  },
  "icon": "assets/images/Bitloops-Icon-256x256.png",
  "license": "MIT",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bitloops",
      "aliases": ["Bitloops", "bitloops"],
      "extensions": [".bl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bitloops",
      "scopeName": "source.bitloops",
      "path": "./syntaxes/bitloops.tmLanguage.json"
    }]
  }
}
